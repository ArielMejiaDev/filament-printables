{
    "name": "fastoficorp/filament-printables",
    "description": "A package to generate reports and form printables for your app",
    "keywords": [
        "FastOfi Corp",
        "laravel",
        "filament-printables"
    ],
    "homepage": "https://github.com/FastofiCorp/filament-printables",
    "license": "MIT",
    "authors": [
        {
            "name": "Alvaro Le√≥n Torres",
            "email": "alvaro.leon@FastofiCorp.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "ticketpark/htmlphpexcel": "^1.0",
        "filament/filament": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },


    "autoload": {
        "psr-4": {
            "FastofiCorp\\FilamentPrintables\\": "src/"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "FastofiCorp\\FilamentPrintables\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FastofiCorp\\filament-printables\\filament-printablesServiceProvider"
            ],
            "aliases": {
                "filament-printables": "FastofiCorp\\filament-printables\\Facades\\filament-printables"
            }
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
